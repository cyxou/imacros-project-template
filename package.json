{
  "name": "imacros-script-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf build && mkdirp build",
    "build": "browserify src/main.js -p [ bannerify --file banner.txt ] -o build/main.js && add-bom --file build/main.js",
    "prewatch": "rimraf build && mkdirp build",
    "watch": "watchify src/main.js -p [ bannerify --file banner.txt ] -o build/main.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cyxou/imacros-script-boilerplate.git"
  },
  "author": "Alex Sukhov",
  "license": "MIT",
  "dependencies": {
    "imacros-play-code": "cyxou/imacros-play-code"
  },
  "devDependencies": {
    "add-bom": "latest",
    "bannerify": "latest",
    "browserify": "latest",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.4.3",
    "watchify": "latest"
  }
}
